#factorial using function:
def fact(n) :

    f = 1

    for i in range(1, n+1):
        f = f * i

    return f

x = 4
value = fact(x)
print(value)



#factorial not using function:
n = int(input('enter the value: '))
result = 1
for i in range(n,0,-1):
    result = result*i
print('factorial of', n,'is', result)

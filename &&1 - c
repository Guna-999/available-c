#include<stdio.h> 
int main()
{
    int s1,s2,s3,s4,s5,per;
    
    printf("enter marks in 5 subjects: ");
    scanf("%d %d %d %d %d", &s1, &s2, &s3, &s4, &s5);
    per=(s1+s2+s3+s4+s5)*100/500;
    
    if(per>=60)
    {
        printf("first division");
    }
    if((per>=50) && (per<60))
    {
        printf("second division");
    }
    if((per>=40) && (per<50))
    {
        printf("third division");
    }
    if(per<40)
    {
        printf("fail\n");
    }
    return 0;
}

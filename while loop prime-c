#include<stdio.h>
int main()
{
    int n,i,count;
    printf("enter the number to check prime or not:");
    scanf("%d",&n);
    i = 2;
    while(i<=n/2)
    {
        if(n%i==0)
        {
            count=2;
            break;
        }
        i++;
    }
    if(count==0)
    {
        printf("%d is prime number.",n);
    }
    else
    {
        printf("%d is not prime number.",n);
    }
    getch();
}

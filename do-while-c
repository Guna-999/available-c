#include<stdio.h>
#include<conio.h>
void main()
{
    int n,f,f1=-1,f2=1;
    printf("enter the number of terms: ");
    scanf("%d",&n);
    printf("the fibonacci series: ");
    do
    {
        f=f1+f2;
        f1=f2;
        f2=f;
        printf("%d",f);
        n--;
    }while(n>0);
   
    getch();
}

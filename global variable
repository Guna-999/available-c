#1
r = 40

def webseries():
    global r
    r = 90
    print('best webseries in netflix :' ,r)

webseries()

print('best in prime :' ,r)



#2
airport = ('land at 23:30')

def flight_007() :
    airport = ('kerala')
    print('landing on sector 7 '+airport)

flight_007()

print(airport)
print('times')





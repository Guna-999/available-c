#1#
#include <stdio.h>
int main()
{
    int a=2, b=3;
    b=a++ + b--;
    a=a-- + ++b;
    b=++a + --b;
    printf("%d,%d",a,b);
}


#2# pre-increment
#include <stdio.h>
int main()
{
    int x = 10, y;
    y = ++x;
    printf("%d,%d",x,y);
}


#3# post-increment
#include <stdio.h>
int main()
{
    int x = 10, y;
    y = x++;
    printf("%d,%d",x,y);
}
